    <validationRules>
        <fullName>End_Date_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c  &gt;= End_Date__c</errorConditionFormula>
        <errorMessage>Start date should not be greater than End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_Rule</fullName>
        <active>true</active>
        <description>if recurring checkbox is true need to fill the Frequency field</description>
        <errorConditionFormula>Recurring__c = True   &amp;&amp;  ISBLANK( TEXT(Frequency__c))</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>Frequency Filed should not be Blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Type_Field_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Event_Type__c, &apos;Virtual&apos;) &amp;&amp; NOT(ISBLANK(Location__c))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>Not Select any Value in Location Field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_type_is_In_Person_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Event_Type__c,&apos;In-Person&apos; )  &amp;&amp;  ISBLANK(Location__c )</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>Please Select Any Of The Location</errorMessage>
    </validationRules>
